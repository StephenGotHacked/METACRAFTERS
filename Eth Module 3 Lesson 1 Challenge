        pragma solidity ^0.8.18;

        contract Metacrafters {

            uint myAge;
            string myFullName;
            bool testing;
            address myCrypto;

        function setMetacrafters(uint Age, string memory myFirstName, string memory myLastName, bool _testing, address _myCrypto )public{
                myAge = Age;
                myFullName = myFirstName; myLastName;
                testing =  _testing;
                myCrypto = _myCrypto;
        }

        function getAge() public view {
                 myAge;
        }

        function getFullName() public view returns (string memory) {
                 return myFullName ;
        }

        function getTest() public view returns (bool) {
                 return testing;
        }

        function getAddress() public view returns (address){
                 return myCrypto;
        }

}        
