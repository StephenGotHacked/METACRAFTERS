        pragma solidity ^0.8.18;

        contract Metacrafters {

            uint myAge;
            string myFullName;
            string myStreetName;
            address myCrypto;


        // a Function method that will holds the set ang get for Unsigned Integers myAge
        function setAge(uint Age)public{
                 myAge = Age;
        }

        function getAge() public view {
                 myAge;
        }


        // a Function method that will holds the set ang get for String myFullname
        function setFullName(string memory myFirstName, string memory myLastName) public{
                 myFullName = myFirstName; myLastName;
        }

        function getFullName() public view returns (string memory) {
                 return myFullName ;
        }


        // a Function method that will holds the set ang get for String myStreeName
        function setStreetName(string memory _streetName) public{
                 myStreetName =  _streetName;
        }

        function getStreetName() public view returns (string memory) {
                 return myStreetName ;
        }

        // a Function method that will holds the set ang get for Address of myCrypto from my Metamask Address
        function setAddress(address _myCrypto) public {
                 myCrypto = _myCrypto;
        }

        function getAddress() public view returns (address){
                 return myCrypto;
        }

}

        
